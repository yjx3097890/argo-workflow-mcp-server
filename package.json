{
  "name": "argo-workflow-mcp-server",
  "version": "1.0.1",
  "description": "MCP Server for managing Argo Workflows via Argo Server API",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "argo-workflow-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testMatch='**/*.property.test.ts'",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "argo-workflows",
    "argo-server",
    "kubernetes",
    "workflow",
    "ai-agent"
  ],
  "author": "yanjixian",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yjx3097890/argo-workflow-mcp-server"
  },
  "bugs": {
    "url": "https://github.com/yjx3097890/argo-workflow-mcp-server/issues"
  },
  "homepage": "https://github.com/yjx3097890/argo-workflow-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "express": "^4.18.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
